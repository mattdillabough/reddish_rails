<%= form_for @user, url: {action: "create"} do |f| %>
  Email: <%= f.text_field(:email) %> <br>
  Username: <%= f.text_field(:username) %> <br>
  Password: <%= f.password_field(:password) %> <br>
  Confirm Password <%= password_field_tag(:password_confirm) %> <br>
  <%= f.submit "Register" %>
<% end %>
